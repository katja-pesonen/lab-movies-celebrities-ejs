<div>
    <h2><%= movie.title %></h2>
    <h3><%= movie.genre %></h3>
    <p><%= movie.plot %></p>
  
    <ul>
      <% movie.cast.forEach((actor) => { %>
      <li><%= actor.name %></li>
  
      <% }) %>
    </ul>
    <a href="/movies/edit/<%= movie.id %>">Edit</a>
    <form action="/movies/delete/<%= movie.id %>" method="POST">
      <button type="submit">Delete</button>
    </form>
  </div>
  
  